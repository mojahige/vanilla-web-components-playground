  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true
  },
  "include": ["decls.d.ts", "components/**/*"]
}
